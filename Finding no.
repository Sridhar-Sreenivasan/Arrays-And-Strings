#include <iostream>
using namespace std;
int main() {
    int arr[] = {11, 889 , 41, 0, 6543};
    int n = 5;
    int target;
    cout << "Enter number to search: ";
    cin >> target;
    for (int i = 0; i < n; ++i) {
        if (arr[i] == target) {
            cout << "Number found in the array." << endl;
            return 0;
        }
    }
    cout << "Number not found in the array." << endl;
    return 0;
}
/*
Output :
Enter number to search: 49
Number not found in the array.
Output :
Enter number to search: 889
Number found in the array.
*/

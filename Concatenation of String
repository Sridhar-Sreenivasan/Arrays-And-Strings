#include <iostream>
using namespace std;
int main(){
    string a,b;
    cout << "enter the first string"<<endl;
    cin>>a;
    cout << "enter the second string"<<endl;
    cin >>b;
    string c ;
    c=a+" "+b;
    cout << "the concatenated string is "<<c<<endl;

    return 0;}


/*OUTPUT:
enter the first string
Sridhar
enter the second string
Sreenivasan
the concatenated string is Sridhar Sreenivasan

*/
